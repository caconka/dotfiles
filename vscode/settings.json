// Coloque su configuración en este archivo para sobrescribir la configuración predeterminada.
{
    "editor.fontFamily": "Monaco",
    "editor.tabSize": 2,
    "editor.lineNumbers": "relative",
    "workbench.colorTheme": "Base16 Ocean Dark",
    "workbench.iconTheme": "vs-seti",
    "terminal.integrated.fontFamily": "Monaco",
    "terminal.integrated.enableBold": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.external.linuxExec": "konsole",
    "vim.disableAnnoyingNeovimMessage": true,
    "workbench.statusBar.visible": true,
    "window.menuBarVisibility": "toggle",
    "window.zoomLevel": 0,
    "editor.multiCursorModifier": "ctrlCmd",
    "vim.disableAnnoyingGcMessage": true,
        "vim.handleKeys":{
        "<C-y>": false
    }
}
